			"core/heading": {
				"spacing": {
					"margin": {
						"bottom": "0",
						"top": "0"
					},
					"padding": {
						"bottom": "0",
						"top": "0"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--30)",
						"top": "var(--wp--preset--spacing--30)"
					},
					"padding": {
						"bottom": "0",
						"top": "0"
					}
				},
				"typography": {
					"fontSize": "16px",
					"lineHeight": "1.7"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"spacing": {
					"padding": {
						"bottom": "0",
						"top": "0"
					}
				},
				"typography": {
					"fontSize": "28px"
				}
			}
		},
